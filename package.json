{
    "name": "tsgenerator",
    "version": "1.0.0",
    "main": "./lib/main.js",
    "typings": "./lib/main.d.ts",
    "scripts": {
        "prepare": "npm run build",
        "build": "tsc",
        "test": "jest"
    },
    "files": [
        "lib"
    ],
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "js"
        ],
        "transform": {
            "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
        },
        "testRegex": "/src/.*\\.spec\\.ts$"
    },
    "author": "Ciaran Courtney",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ciarancourtney/tsgenerator.git"
    },
    "bugs": {
        "url": "https://github.com/ciarancourtney/tsgenerator/issues"
    },
    "description": "Generate deterministic time series data",
    "devDependencies": {
        "@types/jest": "^23.1.0",
        "jest": "^23.1.0",
        "ts-jest": "^22.4.6",
        "typescript": "^2.9.2"
    },
    "dependencies": {
        "moment": "^2.22.2"
    }
}
